{
  "name": "Withdraw as non-owner",
  "comment": "Non-owner tries to withdraw, should fail",
  "steps": [
    {
      "step": "setState",
      "accounts": {
        "owner": {
          "nonce": 0,
          "balance": "1000000000000000000"
        },
        "user": {
          "nonce": 0,
          "balance": "3000000000000000000"
        },
        "attacker": {
          "nonce": 0,
          "balance": "1000000000000000000"
        }
      }
    },
    {
      "step": "scDeploy",
      "txId": "deploy",
      "tx": {
        "from": "owner",
        "contractCode": "file:output/requests-contract.wasm",
        "arguments": [
          "100"
        ],
        "gasLimit": 60000000,
        "gasPrice": 1000000000
      }
    },
    {
      "step": "scCall",
      "txId": "add_requests",
      "tx": {
        "from": "user",
        "to": "sc:deploy",
        "function": "addRequests",
        "arguments": [
          "42"
        ],
        "value": "2000000000000000000",
        "gasLimit": 5000000,
        "gasPrice": 1000000000
      },
      "expect": {
        "status": 0
      }
    },
    {
      "step": "scCall",
      "txId": "withdraw",
      "tx": {
        "from": "attacker",
        "to": "sc:deploy",
        "function": "withdrawAll",
        "arguments": [],
        "gasLimit": 5000000,
        "gasPrice": 1000000000
      },
      "expect": {
        "status": 4,
        "message": "*Only the owner can withdraw*"
      }
    }
  ]
}
